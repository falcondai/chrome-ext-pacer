{
	"name": "Pacer",
	"version": "0.1.0",
	"manifest_version": 2,

	"author": "Falcon Dai",
	"description": "A productivity tool that nudges you every X minutes to help pace yourself.",
	"options_page": "options.html",
	"permissions": [
		"alarms",
		"notifications"
	],
	"background": {
		"persistent": false,
		"page": "background.html"
	},
	"web_accessible_resources": [
		"/assets/image/logo.png"
	]
}
